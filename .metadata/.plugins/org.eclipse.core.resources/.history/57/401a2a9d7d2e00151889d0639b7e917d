import javax.swing.JOptionPane;

public class SimpleNumberSort
{
	public static void main(String[] args)
	{
		int x = JOptionPane.showInputDialog("Type a random number here.");
		int y = JOptionPane.showInputDialog("Type another random number here.");
		int z = JOptionPane.showInputDialog("Type another random number here.");
		
		if(x > y > z)
		{
			System.out.println(z);
			System.out.println(y);
			System.out.println(x);
		}
		if(x < y < z)
		{
			System.out.println(x);
			System.out.println(y);
			System.out.println(z);
		}
		
	}
}
